</lotusscript></code><code event='declarations'><lotusscript>Dim m_docCurr As NotesDocument
Dim m_uidocCurr As NotesUIDocument</lotusscript></code></globals><code event='windowtitle'><formula
>"Landing Page"</formula></code><code event='options'><lotusscript>Option Declare
</lotusscript></code><code event='querysave'><lotusscript>Sub Querysave(Source As Notesuidocument, Continue As Variant)
	
	Const STR_MODULE_NAME = |Landing page profile: Querysave|
	
	Dim docLandingPage As NotesDocument
	Dim y As Integer
	Dim ret As Variant
	Dim x As Integer
	Dim strMsg As String
	Dim intCountErrors As Integer
	Dim strErrorFieldName As String
	
     'Trap errors
	On Error Goto lblError
	
	Call Debug(STR_MODULE_NAME &amp; |: Started.|)
	
	Set docLandingPage = Source.Document
	
	If (docLandingPage.IsNewNote And  flgIsDuplicateLandingPage(m_docCurr)) Then
		Call ShowErrorMsg(|A landing page profile already exists with this landing page id.| &amp; Chr$(13) &amp; Chr$(13) &amp; |Please choose a different landing page id.|)
		Continue = False
		Goto lblExit
	End If
	
	intCountErrors = 0
	strMsg = "The following fields are required to save this document:" &amp; Chr$(13) &amp; Chr$(13)
	
	If (m_docCurr.lp_name(0) = "") Then
		If strErrorFieldName = "" Then
			strErrorFieldName = "lp_name"
		End If
		intCountErrors = intCountErrors + 1
		strMsg = strMsg &amp; " Landing page id" &amp; Chr$(13)
	End If
	
	If (m_docCurr.lp_description(0) = "") Then
		If strErrorFieldName = "" Then
			strErrorFieldName = "lp_description"
		End If
		intCountErrors = intCountErrors + 1
		strMsg = strMsg &amp; " Description" &amp; Chr$(13)
	End If
	
	If (m_docCurr.lp_locale(0) = "") Then
		If strErrorFieldName = "" Then
			strErrorFieldName = "lp_locale"
		End If
		intCountErrors = intCountErrors + 1
		strMsg = strMsg &amp; " Language" &amp; Chr$(13)
	End If
	
	If intCountErrors &gt; 0 Then
		Call ShowErrorMsg(strMsg)
		Source.GotoField(strErrorFieldName)
		Continue = False	
		Goto lblExit
	End If
	
	If Instr(m_docCurr.lp_name(0), " ") &gt; 0 Then
		strMsg = "The Landing page id cannot contain spaces between words. Please remove them."_ 
		&amp; " If you have already distributed the landing page URL you would need to replace with the new URL once you remove the spaces and save this document." &amp; Chr$(13)
		Call ShowErrorMsg(strMsg)
		strErrorFieldName = "lp_name"
		Source.GotoField(strErrorFieldName)
		Continue = False	
		Goto lblExit
	End If
	
	'Build the Landing page query filter on Query Save after the fields have been selected
	
	'Start with a default of all active events
	Dim query As String
	Dim strOR As String
	strOR = " | "
	query  = { Form = "fEventProfile"  &amp; status = "Active"  &amp; ep_IsInvitationOnly != "Yes" &amp; (ep_WebSiteType = "" | ep_WebSiteType = "S") }
	
	If (docLandingPage.lp_QueryLocale(0) &lt;&gt; "") Then
		query  = query  + | &amp; @Contains(ep_Locales;"| + docLandingPage.lp_QueryLocale(0) + |") |
	End If
	
	'Define the potential criteria fields
	Dim criterias(1 To 5) As String
	criterias(1) = "lp_loc_confcountry"
	criterias(2) = "lp_geo"
	criterias(3) = "lp_category"
	criterias(4) = "lp_sponsor_bu_pri"
	criterias(5) = "lp_es_brand"
	
	'Now add any other filters
	Dim strCriteria As String
	
	For y = 1 To 5
		strCriteria = Trim(Source.FieldGetText(criterias(y)))
		Dim intLength As Integer
		Dim strFieldName As String
		intLength = Len(criterias(y))
		intLength = intLength - 3 'Remove the "lp_" prefix
		strFieldName = Right(criterias(y),intLength)
		If ( strCriteria &lt;&gt; "") Then			
			If Instr(strCriteria,",") Then '@@@ Is there several criteria?
				'@@@ Yes, add THEM
				ret = Split(strCriteria,",")
				query  = query + " &amp; ( "
				For x = 0 To Ubound(ret)
					If x &gt; 0 Then ' if second or higher subscript in list then add a OR clause
						query  = query + strOR + | | + strFieldName + | = "| + Trim$(ret(x)) + |" |
					Else  'Otherwise do not
						query  = query + | | + strFieldName + | = "| + Trim$(ret(x)) + |" |
					End If				
				Next
				query  = query + " ) "
			Else '@@@ Nope, just one, add IT
				query  = query + | &amp; | + strFieldName + | = "| + Trim$(strCriteria) + |" |
			End If		
		End If					
	Next
	
	'Update to allow for brand being a multi-value field in the event profile
	strCriteria = Trim(Source.FieldGetText("lp_brand"))
	strFieldName = "brand"
	If (strCriteria &lt;&gt; "") Then			
		If Instr(strCriteria,",") Then 'Are there several criteria?
			'Yes, add them
			ret = Split(strCriteria, ",")
			query  = query + " &amp; ( "
			For x = 0 To Ubound(ret)
				If x &gt; 0 Then ' if second or higher subscript in list then add a OR clause
					query  = query + strOR + | | + |@Contains(| + strFieldName + |; "| + Trim$(ret(x)) + |") |
				Else  'Otherwise do not
					query  = query + | | + |@Contains(| + strFieldName + |; "| + Trim$(ret(x)) + |") |
				End If				
			Next
			query  = query + " ) "
		Else 'Nope, just one, so add it
			query  = query + | &amp; | + |@Contains(| + strFieldName + |; "| + Trim$(strCriteria) + |") |
		End If		
	End If					
	
	docLandingPage.lp_query = query
	
	'Build graphic table using commons function
	'Build the graphic table for the landing page
	docLandingPage.lp_graphicTableComputed = getGraphicTable(docLandingPage)
	
	
lblExit:
	
	Call Debug(STR_MODULE_NAME &amp; |: Finished.|)
	
	Exit Sub
	
lblError:
	
	Call Debug(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Call LogError(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Resume lblExit
	
End Sub
</lotusscript></code><code event='postopen'><lotusscript>Sub Postopen(Source As Notesuidocument)
	
	Set m_uidocCurr = Source
	Set m_docCurr = m_uidocCurr.Document	
	
End Sub
</lotusscript></code><code event='queryopen'><lotusscript>Sub Queryopen(Source As Notesuidocument, Mode As Integer, Isnewdoc As Variant, Continue As Variant)
	
	Const STR_MODULE_NAME = |Landing page profile: Queryopen|
	
	Dim doc As NotesDocument
	Dim flgAllowedToEdit As Integer
	
	Call Debug(STR_MODULE_NAME &amp; |: Started.|)
	
	Set doc = Source.Document
	
	flgAllowedToEdit = flgCanEdit(doc, g_strUserName)
	
	'Check that the user is allowed to edit the document and display a message and exit they are not allowed.
	If (Not flgAllowedToEdit) Then
		Call ShowErrorMsg(|You are not allowed to edit a default landing page.| &amp; Chr$(13) &amp; Chr$(13) &amp; |To create a new landing page click the 'Add Landing Page Configuration' button.|)
		Call Debug(STR_MODULE_NAME &amp; |: [| &amp; g_strUserName &amp; |] attempted to edit a default landing page.|)
		Continue = False
	End If     
	
	Call Debug(STR_MODULE_NAME &amp; |: Finished.|)
	
End Sub
</lotusscript></code><code event='querymodechange'><lotusscript>Sub Querymodechange(Source As Notesuidocument, Continue As Variant)
	
	Const STR_MODULE_NAME = |Landing page profile: Querymodechange|
	
	Dim doc As NotesDocument
	Dim flgAllowedToEdit As Integer
	
	Call Debug(STR_MODULE_NAME &amp; |: Started.|)
	
	Set doc = Source.Document
	
	flgAllowedToEdit = flgCanEdit(doc, g_strUserName)
	
	If Not Source.EditMode And (Not flgAllowedToEdit) Then
		Call ShowErrorMsg(|You are not allowed to edit a default landing page.| &amp; Chr$(13) &amp; Chr$(13) &amp; |To create a new landing page click the 'Add Landing Page Configuration' button.|)
		Call Debug(STR_MODULE_NAME &amp; |: [| &amp; g_strUserName &amp; |] attempted to edit a default landing page.|)
		Continue = False
	End If     
	
	Call Debug(STR_MODULE_NAME &amp; |: Finished.|)
	
End Sub
</lotusscript></code>
