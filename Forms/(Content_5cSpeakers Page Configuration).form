End Sub
</lotusscript></code><code event='querysave'><lotusscript>Sub Querysave(Source As Notesuidocument, Continue As Variant)
	
	Const STR_MODULE_NAME = |Speakers page configuration: Querysave|
	
	Dim strMsg As String
	Dim intCountErrors As Integer
	Dim strErrorFieldName As String
	
     'Trap errors
	On Error Goto lblError
	
	intCountErrors = 0
	strMsg = "The following fields are required to save this document:" &amp; Chr$(13) &amp; Chr$(13)
	
	'If the preface section is included, check that one has been entered.
	If (m_docCurr.includeabs(0) = "Yes") Then
		Call CheckField(m_docCurr, |abs_info|, |Preface text|, strMsg, strErrorFieldName, intCountErrors)
		Call CheckField(m_docCurr, |sk_SectionTitle|, |Section title|, strMsg, strErrorFieldName, intCountErrors)		
	End If
	
	Call CheckField(m_docCurr, |speakercountchoice|, |How may speakers?|, strMsg, strErrorFieldName, intCountErrors)
	
lblReportErrors:
	
	If intCountErrors &gt; 0 Then
		Call ShowErrorMsg(strMsg)
		Source.GotoField(strErrorFieldName)
		Continue = False	
		Goto lblExit
	End If
	
	'Validate that at least one speaker is selected
	If (m_docCurr.speakercountchoice(0) &lt;&gt; "") Then
		If Not (m_docCurr.speakername1(0) &lt;&gt; "" Or m_docCurr.speakername2(0) &lt;&gt; "" Or m_docCurr.speakername3(0) &lt;&gt; "" Or m_docCurr.speakername4(0) &lt;&gt; "" Or m_docCurr.speakername5(0) &lt;&gt; "" Or m_docCurr.speakername6(0) &lt;&gt; "" Or m_docCurr.speakername7(0) &lt;&gt; "" Or m_docCurr.speakername8(0) &lt;&gt; "" Or m_docCurr.speakername9(0) &lt;&gt; "" Or m_docCurr.speakername10(0) &lt;&gt; "") Then
			Call ShowErrorMsg(|Please select at least one speaker.|)
			Continue = False
			Goto lblExit			
		End If
	End If
	
lblExit:
	
	Exit Sub
	
lblError:
	
	'Workaround to prevent errors -28363 and 4412 which may results from a Notes bug when using GotoField or FieldGetText 
	'See Notes:///85256CC90081C6F2/B48A95F9B4A7A68185256C8D0072E04C/3667EBDC0B605D0685257241004E19B3 in the Lotus KnowledgeBase
	If Err() = -28363 Then
		Resume Next
	End If
	If Err() = 4412 Then
		Resume Next
	End If	
	Call Debug(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Call LogError(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Resume lblExit
	
End Sub
</lotusscript></code>
