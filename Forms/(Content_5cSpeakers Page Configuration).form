<globals><code event='options'><lotusscript>Option Public
Option Declare

Use "Common Functions"

</lotusscript></code><code event='declarations'><lotusscript>Dim m_docCurr As NotesDocument
Dim m_uidocCurr As NotesUIDocument

</lotusscript></code><code event='SelectSpeaker'><lotusscript>Sub SelectSpeaker(intSpeakerNumber As Integer)
	
	Const STR_MODULE_NAME = |Speakers page configuration: SelectSpeaker|
	
	Dim ws As New NotesUIWorkspace	
	Dim col As NotesDocumentCollection
	Dim docSpeakerBio As NotesDocument
	Dim strSpeakerNameField As String
	Dim strSpeakerBioUNIDField As String
	
     'Trap errors
	On Error Goto lblError
	
	Set col = ws.PickListCollection( PICKLIST_CUSTOM, False, g_dbCurr.Server, g_dbCurr.FilePath, "vDialogSelectSpeakerBio", "Select a speaker", "Please select a speaker from the list")
	
	If (col.count &gt; 0) Then
		Set docSpeakerBio = col.GetFirstDocument
		strSpeakerNameField = |speakername| &amp; Cstr(intSpeakerNumber)
		strSpeakerBioUNIDField = |speaker| &amp; Cstr(intSpeakerNumber)		
		Call m_docCurr.ReplaceItemValue(strSpeakerNameField, docSpeakerBio.name(0))
		Call m_docCurr.ReplaceItemValue(strSpeakerBioUNIDField, docSpeakerBio.docid(0))
		Call m_uidocCurr.Reload
		Call m_uidocCurr.Refresh
	End If
	
lblExit:
	
	Exit Sub
	
lblError:
	
	Call Debug(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Call LogError(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Resume lblExit
	
End Sub
</lotusscript></code><code event='RemoveSpeaker'><lotusscript>Sub RemoveSpeaker(intSpeakerNumber As Integer)
	
	Const STR_MODULE_NAME = |Speakers page configuration: RemoveSpeaker|
	
	Dim strSpeakerNameField As String
	Dim strSpeakerBioUNIDField As String
	
     'Trap errors
	On Error Goto lblError
	
	strSpeakerNameField = |speakername| &amp; Cstr(intSpeakerNumber)
	strSpeakerBioUNIDField = |speaker| &amp; Cstr(intSpeakerNumber)		
	Call m_docCurr.ReplaceItemValue(strSpeakerNameField, ||)
	Call m_docCurr.ReplaceItemValue(strSpeakerBioUNIDField, ||)
	Call m_uidocCurr.Reload
	Call m_uidocCurr.Refresh
	
lblExit:
	
	Exit Sub
	
lblError:
	
	Call Debug(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Call LogError(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Resume lblExit
	
End Sub</lotusscript></code></globals><code event='windowtitle'><formula>"Speakers: " + eventname</formula></code><code
 event='options'><lotusscript>Option Declare

</lotusscript></code><code event='queryopen'><lotusscript>Sub Queryopen(Source As Notesuidocument, Mode As Integer, Isnewdoc As Variant, Continue As Variant)
	
	Const STR_MODULE_NAME = |Speakers page configuration: Queryopen|
	
	Dim doc As NotesDocument
	Dim flgAllowedToEdit As Integer
	Dim nameOwner As NotesName
	Dim strOwnerName As String
	Dim docEventProfile As NotesDocument
	
     'Trap errors
	On Error Goto lblError
	
	Set doc = Source.Document
	
	flgAllowedToEdit = flgCanEdit(doc, g_strUserName)
	
	'Check that the user is allowed to edit the document and display a message and exit if they are not allowed.
	If (Not flgAllowedToEdit) Then
		Set docEventProfile = docGetEventProfileByEventID(Cstr(doc.ep_id(0)))
		If (Not docEventProfile Is Nothing) Then
			Set nameOwner = New NotesName(Cstr(docEventProfile.offeringQuestionsName(0)))
			strOwnerName = nameOwner.Abbreviated	
			Call ShowErrorMsg(|Access to this document has been restricted.  Please contact | &amp; strOwnerName &amp; |, the event owner, to request access.|)
			Call Debug(STR_MODULE_NAME &amp; |: [| &amp; g_strUserName &amp; |] attempted to edit the speakers form with UNID [| &amp; doc.UniversalID &amp; |] but was not listed as an allowable editor.|)
			Continue = False
		Else
			Call ShowErrorMsg(|The system could not find the event profile associated with this event.  Please contact the system administrator.|)
			Call Debug(STR_MODULE_NAME &amp; |: Unable to find event profile.|)
			Continue = False
		End If
	End If     
	
lblExit:
	
	Exit Sub
	
lblError:
	
	Call Debug(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Call LogError(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Resume lblExit
	
End Sub
</lotusscript></code><code event='querymodechange'><lotusscript>Sub Querymodechange(Source As Notesuidocument, Continue As Variant)
	
	Const STR_MODULE_NAME = |Speakers page configuration: Querymodechange|
	
	Dim doc As NotesDocument
	Dim flgAllowedToEdit As Integer
	Dim nameOwner As NotesName
	Dim strOwnerName As String
	Dim docEventProfile As NotesDocument
	
	'Trap errors
