</lotusscript></code><code event='StoreInitialValues'><lotusscript>Sub StoreInitialValues()
     '------------------------------------------------------------------------------------------------------
     'Description
     '------------------
     'Store the initial value of the fields that we need to know have
	'been changed during an edit session.
	'
     'Parameters
     '-----------------------
     'None
     '     
     'Return Values
     '-----------------------
     'None
     '------------------------------------------------------------------------------------------------------   
	
	Const STR_MODULE_NAME = |Invitation: StoreInitialValues|
	
	'Trap errors
	On Error Goto lblError
	
	'Store the initial values of any of the fields that, if changed, mean that we need to
	'take some action when saving the document
	m_strInvitationStatus = Cstr(m_docCurr.in_status(0))
	m_strResponseStatus = Cstr(m_docCurr.in_RSVPStatus(0))
	
lblExit:
	
	Exit Sub
	
lblError:
	
	Call Debug(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Call LogError(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Resume lblExit
	
End Sub
</lotusscript></code><code event='flgFieldValueHasChanged'><lotusscript>Function flgFieldValueHasChanged(strOriginalValue As String, strCurrValue As String, strFieldTitle As String) As Boolean
	
	Const STR_MODULE_NAME = |Invitation: flgFieldHasChanged|
	
	Dim flgHasChanged As Boolean
	
	'Trap errors
	On Error Goto lblError
	
	flgHasChanged = False
	
	If (strOriginalValue &lt;&gt; strCurrValue) Then
		flgHasChanged = True
		Call Debug(STR_MODULE_NAME &amp; |: [| &amp; strFieldTitle &amp; |] has changed  from [| &amp; strOriginalValue &amp; |] to [| &amp; strCurrValue &amp; |].|)
	End If
	
lblExit:
	
	flgFieldValueHasChanged = flgHasChanged
	
	Exit Function
	
lblError:
	
	'Safest to assume field value has changed if there's an error
	flgHasChanged = True
	Call Debug(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Call LogError(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Resume lblExit
	
End Function
</lotusscript></code><code event='OpenRegistrationRecord'><lotusscript>Sub OpenRegistrationRecord()
     '------------------------------------------------------------------------------------------------------
     'Description
     '------------------
     'Open the enrollment document associated with this invitation.
	'
     'Parameters
     '-----------------------
     'None
     '     
     'Return Values
     '-----------------------
     'None
     '------------------------------------------------------------------------------------------------------   
	
	Const STR_MODULE_NAME = |Invitation: OpenRegistrationRecord|
	
	Dim ws As New NotesUIWorkspace		
	Dim strUNID As String
	Dim docEnrollment As NotesDocument
	
	'Trap errors
	On Error Goto lblError
	
	strUNID = m_docCurr.en_UNID(0)
	
	If (strUNID = ||) Then
		Call ShowInfoMsg(|The registration that is associated with this invitation could not be found.|)
	Else
		Set docEnrollment = g_dbCurr.GetDocumentByUNID(strUNID)
		Call ws.EditDocument(True, docEnrollment)	
	End If
	
lblExit:
	
	Exit Sub
	
lblError:
	
	Call Debug(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Call LogError(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Resume lblExit
	
End Sub</lotusscript></code></globals><code event='windowtitle'><formula>@If(en_first = ""; "New invitation"; "Invitation for " + en_first + " " + en_last + " (" + en_email + ")")</formula></code><code
 event='options'><lotusscript>Option Declare

</lotusscript></code><code event='queryopen'><lotusscript>Sub Queryopen(Source As Notesuidocument, Mode As Integer, Isnewdoc As Variant, Continue As Variant)
	
	Const STR_MODULE_NAME = |Invitation: Queryopen|
	
	Call Debug(STR_MODULE_NAME &amp; |: Started.|)
	
     'Trap errors
	On Error Goto lblError
	
	Dim docCurr As NotesDocument
	Dim docLocaleProfile As NotesDocument
	Dim itemFieldValues As NotesItem
	Dim strLocale As String
	
	Set docCurr = Source.Document
	
	strLocale = docCurr.in_Locale(0)
	Call Debug(STR_MODULE_NAME &amp; |: Locale: | &amp; strLocale)		
	
	Set docLocaleProfile = docGetLocaleProfile(strLocale)
	
	Set itemFieldValues = docLocaleProfile.GetFirstItem( "lc_AttendeeTypeValues")
	Call docCurr.ReplaceItemValue("lc_AttendeeTypeValues",itemFieldValues.Values)
	
	Set itemFieldValues = docLocaleProfile.GetFirstItem( "lc_PrefixValues")
	Call docCurr.ReplaceItemValue("lc_PrefixValues",itemFieldValues.Values)
	
	Set itemFieldValues = docLocaleProfile.GetFirstItem( "lc_CountryValues")
	Call docCurr.ReplaceItemValue("lc_CountryValues",itemFieldValues.Values)
	
lblExit: 
	
	Call Debug(STR_MODULE_NAME &amp; |: Finished.|)
	
	Exit Sub
	
lblError:
	
	Call Debug(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Call LogError(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Resume lblExit
	
End Sub
</lotusscript></code><code event='querysave'><lotusscript>Sub Querysave(Source As Notesuidocument, Continue As Variant)
	
	Const STR_MODULE_NAME = |Invitation: Querysave|
	
	Dim strMsg As String
	Dim intCountErrors As Integer
	Dim strErrorFieldName As String
	Dim docInvitation As NotesDocument
	Dim docLocaleProfile As NotesDocument
	Dim strEventID As String
	Dim strLocale As String
	Dim strInvitationID As String
	
     'Trap errors
	On Error Goto lblError
	
	intCountErrors = 0
	strMsg = "The following fields are required to save this document:" &amp; Chr$(13) &amp; Chr$(13)
	
	'Validate the required fields in the order they appear on the form
	If (Source.FieldGetText("in_Status") = "") Then
		If strErrorFieldName = "" Then
			strErrorFieldName = "in_Status"
		End If
		intCountErrors = intCountErrors + 1
		strMsg = strMsg &amp; " Invitation status" &amp; Chr$(13)
	End If
	
	If (Source.FieldGetText("in_RSVPStatus") = "") Then
		If strErrorFieldName = "" Then
			strErrorFieldName = "in_RSVPStatus"
		End If
		intCountErrors = intCountErrors + 1
		strMsg = strMsg &amp; " Reply status" &amp; Chr$(13)
	End If
	
'	Changed 09/18/2008 to allow attendee type to be blank
	
'	If (Source.FieldGetText("en_attendeetype") = "") Then
'		If strErrorFieldName = "" Then
'			strErrorFieldName = "en_attendeetype"
'		End If
'		intCountErrors = intCountErrors + 1
'		strMsg = strMsg &amp; " Attendee type" &amp; Chr$(13)
'	End If
	
	If (Source.FieldGetText("en_first") = "") Then
		If strErrorFieldName = "" Then
			strErrorFieldName = "en_first"
		End If
		intCountErrors = intCountErrors + 1
		strMsg = strMsg &amp; " First name" &amp; Chr$(13)
	End If
	
	If (Source.FieldGetText("en_last") = "") Then
		If strErrorFieldName = "" Then
			strErrorFieldName = "en_last"
		End If
		intCountErrors = intCountErrors + 1
		strMsg = strMsg &amp; " Last name" &amp; Chr$(13)
	End If
	
	If (Source.FieldGetText("en_companyname") = "") Then
		If strErrorFieldName = "" Then
			strErrorFieldName = "en_companyname"
		End If
		intCountErrors = intCountErrors + 1
		strMsg = strMsg &amp; " Company" &amp; Chr$(13)
	End If
	
	If (Source.FieldGetText("en_email") = "") Then
		If strErrorFieldName = "" Then
			strErrorFieldName = "en_email"
		End If
		intCountErrors = intCountErrors + 1
		strMsg = strMsg &amp; " E-mail" &amp; Chr$(13)
	End If
	
	If intCountErrors &gt; 0 Then
		Call ShowErrorMsg(strMsg)
		Source.GotoField(strErrorFieldName)
		Continue = False	
		Goto lblExit
	End If
	
	Set docInvitation = Source.Document
	
	'Get the event ID and locale	
	strEventID = docInvitation.ep_id(0)
	strLocale = docInvitation.in_Locale(0)
	
	'If the invitation is a duplicate then display an error message
	If (flgIsDuplicateInvitation(docInvitation)) Then
		docInvitation.SaveOptions = "0"
		Call Debug(STR_MODULE_NAME &amp; |: Duplicate invitation prevented for [| &amp; Cstr(docInvitation.en_email(0)) &amp; |].|)	
		Call ShowErrorMsg(|An invitation already exists for | &amp; Cstr(docInvitation.en_email(0)) &amp; |.|)
		Continue = False			
		Goto lblExit
	End If	
	
	'Get the locale profile from the reference database
	Set docLocaleProfile = docGetLocaleProfile(strLocale)
	
	'Now translate each field that needs translation
	docInvitation.en_attendeetype_trans = strGetTranslation(docLocaleProfile, Cstr(docInvitation.en_attendeetype(0)), |lc_AttendeeTypeValues|)
	
	'Remove any translations that are not needed
	Call docInvitation.RemoveItem(|lc_AttendeeTypeValues|)
	
	'Store the invitation ID if not already available
	If (docInvitation.in_id(0) = ||) Then
		strInvitationID = Lcase$(Mid$(docInvitation.UniversalID, 7, 6))
		docInvitation.in_id = strInvitationID
	End If
	
lblExit:
	
	Exit Sub
	
lblError:
	
	Call Debug(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Call LogError(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Resume lblExit
	
End Sub
</lotusscript></code><code event='postopen'><lotusscript>Sub Postopen(Source As Notesuidocument)
	
	Set m_uidocCurr = Source
	Set m_docCurr = m_uidocCurr.Document	
	
	'Store the values of the fields, that if changed, require action when the document is saved
	Call StoreInitialValues()
	
End Sub
</lotusscript></code><code event='postsave'><lotusscript>Sub Postsave(Source As Notesuidocument)
	
	Const STR_MODULE_NAME = |Invitation: Postsave|
	
	Dim ws As New NotesUIWorkspace
	Dim strCurrInvitationStatus As String
	Dim strCurrResponseStatus As String
	Dim strInviteeName As String
	Dim dtNow As New NotesDateTime("")		
	Dim strUNID As String
	Dim docEnrollment As NotesDocument
	Dim strOriginalEnrollmentStatus As String
	
     'Trap errors
	On Error Goto lblError
	
	strCurrInvitationStatus = Cstr(m_docCurr.in_Status(0))
	strCurrResponseStatus = Cstr(m_docCurr.in_RSVPStatus(0))
	strInviteeName = Cstr(m_docCurr.en_first(0)) &amp; | | &amp; Cstr(m_docCurr.en_last(0))
	
	'Set the date/time used to timestamp the document
	dtNow.SetNow
	
	'If the invitation status has changed to 'Invited', store a timestamp
	If (flgFieldValueHasChanged(m_strInvitationStatus, strCurrInvitationStatus, |Invitation status|)) Then
		If (strCurrInvitationStatus = |Invited|) Then
			Set m_docCurr.in_DateInvited = dtNow
			Call m_docCurr.Save(True, False)
			'Refresh the view that the document was opened from so that the status change is visible in the view
			Call ws.ViewRefresh
		Else
			'If the invitation status has changed from 'Invited', reset the timestamp	
			If (strCurrInvitationStatus &lt;&gt; |Invited|) Then
				Call m_docCurr.RemoveItem(|in_DateInvited|)
				Call m_docCurr.Save(True, False)				
				'Refresh the view that the document was opened from so that the status change is visible in the view
				Call ws.ViewRefresh				
			End If
		End If		
	End If
	
	'If the response status has changed to 'Accepted' or 'Declined', store a timestamp	
	If (flgFieldValueHasChanged(m_strResponseStatus, strCurrResponseStatus, |Response status|)) Then
		If (strCurrResponseStatus = |Accepted| Or strCurrResponseStatus = |Declined|) Then
			Set m_docCurr.in_DateResponseReceived = dtNow	
			Call m_docCurr.Save(True, False)			
			'Refresh the view that the document was opened from so that the status change is visible in the view
			Call ws.ViewRefresh			
			
			'If there is a registration record, let the user know so that they can update it.
			strUNID = m_docCurr.en_UNID(0)
			If (strUNID &lt;&gt; ||) Then
				Set docEnrollment = g_dbCurr.GetDocumentByUNID(strUNID)
				strOriginalEnrollmentStatus = docEnrollment.ad_FormStatus(0)			
				Call ShowInfoMsg(|A registration record for | &amp; strInviteeName &amp; | has been found.  The current status of the registration record is '| &amp; strOriginalEnrollmentStatus &amp; |'.| &amp; Chr$(13) &amp; Chr$(13) &amp; |Any changes that you make to an invitation are not automatically made to the associated registration record.  If you want to change the status of the registration record, find the registration record in any of the registration views and use the 'Attended', 'Reset to Confirmed' or 'Cancelled' buttons to change the status.|)
			End If
			
		Else
			'If the response status has changed to 'No response', reset the timestamp	
			If (strCurrResponseStatus = |No response|) Then
				Call m_docCurr.RemoveItem(|in_DateResponseReceived|)
				Call m_docCurr.Save(True, False)				
				'Refresh the view that the document was opened from so that the status change is visible in the view
				Call ws.ViewRefresh			
				
				'Check whether a registration record already exists and if it does, remind the user to update it.
				strUNID = m_docCurr.en_UNID(0)
				If (strUNID &lt;&gt; ||) Then
					Set docEnrollment = g_dbCurr.GetDocumentByUNID(strUNID)
					strOriginalEnrollmentStatus = docEnrollment.ad_FormStatus(0)						
					Call ShowInfoMsg(|A registration record for | &amp; strInviteeName &amp; | has been found.  The current status of the registration record is '| &amp; strOriginalEnrollmentStatus &amp; |'.| &amp; Chr$(13) &amp; Chr$(13) &amp; |Any changes that you make to an invitation are not automatically made to the associated registration record.  If you want to change the status of the registration record, find the registration record in any of the registration views and use the 'Attended', 'Reset to Confirmed' or 'Cancelled' buttons to change the status.|)
				End If
			End If
		End If
	End If
	
lblExit:
	
	Exit Sub
	
lblError:
	
	Call Debug(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Call LogError(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Resume lblExit
	
End Sub
</lotusscript></code>
