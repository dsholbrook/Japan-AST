 event='windowtitle'><formula>tmp := @If(el_type = "Graphic"; "Graphic"; el_type = "PDF"; "PDF File"; el_type = "MP3"; "MP3 File"; el_type = "Video"; "Video file";el_type = "HTML"; "HTML Element"; el_type = "Text"; "Text Element"; "Element");
@If(@IsNewDoc; "New " + tmp; tmp + ": " + el_description)</formula></code><code
 event='querysave'><lotusscript>Sub Querysave(Source As Notesuidocument, Continue As Variant)
	
	Dim doc As notesdocument
	Set doc = source.document
	
	Dim count As Integer
	Dim message As String
	
	Dim strType As String
	
	message = "Please enter the following required field(s): "
	count = 0
	
	strType = Trim(doc.getitemvalue("el_type")(0))
	
	'Extract the first attachment file name, remove the .pdf extension and stick it in the el_key if this is a PDF or MP3
	If strType = "PDF" Or strType = "MP3"  Or strType = "Video" Then
		
		
	Else		
		' Only check validate el_key if not PDF
		If Trim(doc.getitemvalue("el_key")(0)) = "" Then	
			count = count + 1
			message = message + " Element Key"	
		End If
	End If	
	
	If Trim(doc.getitemvalue("el_description")(0)) = "" Then	
		If count &gt; 0 Then
			message = message + ", Description"
		Else
			message = message + "Description"
		End If
		count = count + 1		
	End If
	
	
	If (strType = |Video|) Then
		
		Dim totalSize As Long
		
		Set doc = Source.document
		
		Call Source.refresh(True)
		Dim rtitem As Variant
		Dim embedObjs As Variant
		
		Set rtitem = doc.getFirstItem("el_elementRT")
		embedObjs = rtitem.embeddedObjects
		totalSize = 0
		Dim oneMB As Long
		oneMB = 1048576
		
		
		If Not Isempty(embedObjs) Then
			If Ubound(embedObjs)&gt;0 Then
				Print Ubound(embedObjs)
				Msgbox("Only one (1) file may be attach per document, please delete extra files and re-save.")
				Continue = False
			End If
			For i = 0 To Ubound(embedObjs)
				totalSize = totalSize + embedObjs(i).filesize
				If embedObjs(i).filesize &gt; 3 * oneMB Then
					Msgbox("Your attachment '"  +embedObjs(i).name +"' is " +Cstr(Round(embedObjs(i).filesize / oneMB,1)) +" mb. The limit is 3.0 mb.")
					Continue = False						
				End If
			Next
			
			'If totalSize &gt; 4 * oneMB Then
				'Msgbox("Your total attachment size is " +Cstr(Round (totalSize / oneMB,1)) +" mb.  The limit is 4.0 mb.")
				'Continue = False					
			'End If
			
		End If
		
	End If
	
	If (strType = |PDF| Or strType = |MP3| Or strType = |Graphic|) Then		
		
		If Trim(doc.getitemvalue("el_ALT")(0)) = "" Then	
			If count &gt; 0 Then
				message = message + ", Alt text"
			Else
				message = message + "Alt text"
			End If
			count = count + 1		
		End If
	End If
	
	If Trim(doc.getitemvalue("el_type")(0)) = "" Then	
		If count &gt; 0 Then
			message = message + ", Type"
		Else
			message = message + "Type"
		End If
		count = count + 1		
	End If
	
	If count &gt; 0 Then
		answer% = Messagebox(message, 0 + 16, "Element Information")
		continue = False	
	End If
End Sub
</lotusscript></code>
