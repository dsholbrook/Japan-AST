<par def='477'><run><font size='9pt' name='Arial' pitch='variable' truetype='true'
 familyid='20' color='gray'/>To send the registration records to CRM Siebel via the Web to CRM process click </run><actionhotspot
 hotspotstyle='none'><run><font size='9pt' style='underline' name='Arial'
 pitch='variable' truetype='true' familyid='20' color='#3c5e84'/>Submit to CRM Siebel</run><code
 event='click'><lotusscript>Sub Click(Source As Button)
	
	Dim strMsg As String
	Dim intCountErrors As Integer
	Dim flgOK As Boolean
	Dim dtNow As New NotesDateTime("")
	
	'Set the date/time used to timestamp the update
	dtNow.SetNow	
	
	flgOK = False
	intCountErrors = 0 
	
	If m_docCurr.ep_SendToCRM(0)="No"  Then
		strMsg  = strMsg &amp; Chr(13) &amp;  "- You answered No to the question 'Do you want the event data be sent to Web2CRM?'."	
		intCountErrors = intCountErrors +1
	End If
	
	If m_docCurr.ep_WarmTacticCode(0)= ""  Then
		strMsg  = strMsg &amp; Chr(13) &amp;  "- No 'Proactive - WARM' tactic code was supplied."	
		intCountErrors = intCountErrors +1
	End If
	
	If m_docCurr.ep_HotTacticCode(0)= ""  Then
		strMsg  = strMsg &amp; Chr(13) &amp;  "- No 'Reactive - HOT' tactic code was supplied."	
		intCountErrors = intCountErrors +1
	End If
	
	'Check if any errors were detected
	If intCountErrors &gt; 0 Then
		strMsg  = |The following items are missing or need correction:|  &amp; Chr(13) &amp; strMsg &amp; Chr(13) &amp; Chr(13) &amp; |Please correct and re-submit.|
		Call ShowErrorMsg(strMsg)
	Else
		flgOK = True
	End If
	
	If (flgOK) Then
		Call m_docCurr.ReplaceItemValue("ep_ReadyForWeb2CRM", "Yes")
		'Add a timestamp to indicate when the update occurred
		Set m_docCurr.ep_DateSubmittedToCRM = dtNow			
		m_docCurr.ep_SubmittedToCRMBy = g_strUserName	
		m_docCurr.ep_SubmittedToWeb2CRMBy = g_strUserName + " on "+ Date$ + " at " + Time$
		m_docCurr.ep_WebToCRMHistory = m_docCurr.ep_WebToCRMHistory(0) + Chr$(13) + "Marked as ready to send registration records to CRM Siebel by " + Cstr(m_docCurr.ep_SubmittedToWeb2CRMBy(0)) + "."
		Call UpdateAudit(m_docCurr, g_strCommonUserName, |Ready to send registration records to CRM Siebel.|)		
		Call m_docCurr.Save(True, False)
		Call ShowInfoMsg("The registration records will be sent to CRM Siebel through the Web to CRM process. It takes up to 72 hours (3 business days) to process the records and for CRM Siebel to pick up the data.")
		Call m_uidocCurr.Refresh
	End If
	
End Sub
</lotusscript></code></actionhotspot><run><font size='9pt' name='Arial' pitch='variable'
 truetype='true' familyid='20' color='gray'/>.</run></par>
