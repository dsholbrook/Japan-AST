<par def='476'><run><font size='9pt' name='Arial' pitch='variable' truetype='true'
 familyid='20' color='gray'/>2. Click </run><actionhotspot hotspotstyle='none'><run
><font size='9pt' style='underline' name='Arial' pitch='variable' truetype='true'
 familyid='20' color='#3c5e84'/>Attendance update is complete</run><code event='click'><lotusscript
>Sub Click(Source As Button)
	
	Dim strFormula As String
	Dim collRegistrations As NotesDocumentCollection
	Dim dtNow As New NotesDateTime("")
	
	'Set the date/time used to timestamp the update
	dtNow.SetNow	
	
	strFormula = "@Contains(Form; 'enrollall') &amp; ad_formstatus = 'Attended' &amp; ep_id = '" + m_docCurr.ep_id(0) + "'"
	
	Set collRegistrations = g_dbCurr.Search(strFormula, Nothing, 0)
	
	If collRegistrations.Count = 0 Then
		Call ShowErrorMsg(|Attendance update cannot be set as complete because no registration records were found marked as 'Attended'.| &amp; Chr$(13) &amp; Chr$(13) &amp; |Please mark at least one registration record as 'Attended'.|)
	Else
		'Add a timestamp to indicate when the update occurred
		Set m_docCurr.ep_DateAttendanceUpdateComplete = dtNow		
		m_docCurr.ep_AttendanceUpdateCompleteBy = g_strUserName	
		m_docCurr.ep_AttendanceUpdateStatus = "Completed"
		m_docCurr.ep_AttendanceUpdateMarkedCompleteBy = g_strUserName + " on "+ Date$ + " at " + Time$
		m_docCurr.ep_IsAttendanceOverdue = ""
		m_docCurr.ep_DateAttendanceMarkedOverdue = ""		
		Call UpdateAudit(m_docCurr, g_strCommonUserName, |Attendance marked complete.|)
		Call m_docCurr.Save(True, False)
		Call m_uidocCurr.Reload
		Call m_uidocCurr.Refresh
	End If
	
End Sub
</lotusscript></code></actionhotspot><run><font size='9pt' name='Arial' pitch='variable'
 truetype='true' familyid='20' color='gray'/>.</run></par>
