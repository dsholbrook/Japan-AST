<par def='456'><run><font size='9pt' name='Arial' pitch='variable' truetype='true'
 familyid='20' color='gray'/>3. Click here ----&gt; </run><actionhotspot hotspotstyle='none'><run
><font size='9pt' style='underline' name='Arial' pitch='variable' truetype='true'
 familyid='20' color='#3c5e84'/>Attendance update is complete</run><code event='click'><lotusscript
>Sub Click(Source As Button)
	
	Dim ws As New NotesUIWorkspace
	Dim strFormula As String, strTactic As String, strEventID As String
	Dim collRegistrations As NotesDocumentCollection
	Dim docDialog As NotesDocument
	Dim flgOk As Integer
	Dim dtNow As New NotesDateTime("")
	Dim dtToday As NotesDateTime
	Dim dtEndDate As NotesDateTime
	Dim strEndDate As String
	
	strTactic = m_docCurr.tactic(0)
	strEventID = Trim$(m_docCurr.ep_id(0))
	
	'Set the date/time used to timestamp the update
	dtNow.SetNow
	
	'Get today's date
	Set dtToday = New NotesDateTime("Today")
	
	'Get the event end date
	strEndDate = m_docCurr.ep_EndDate(0)
	Set dtEndDate = New NotesDateTime(strEndDate)
	If dtToday.TimeDifference(dtEndDate) &lt;= 0  Then
		Call ShowErrorMsg(|This event has not taken place yet, you cannot mark attendance completed.|)
		Exit Sub
	End If
	
	'Only check for EST briefs, not Other Events
	If Right(strEventID, 2) = "ES" Then
		'Check that brief is using project codes
		If m_docCurr.PLANMGMTAPPLCD(0) &lt;&gt; "X" Then
			Call ShowErrorMsg(|Attendance cannot be marked complete because the brief is not setup to use project codes.|)
			Goto lblExit		
		End If
		
		'Check to make sure project code is not blank
		If strTactic = "" Then
			Call ShowErrorMsg(|Attendance cannot be marked complete because there is no project code set up on the brief.|)
			Goto lblExit		
		End If
	End If
	
	strFormula = "@Contains(Form; 'enrollall') &amp; ad_formstatus = 'Attended' &amp; ep_id = '" + m_docCurr.ep_id(0) + "'"
	
	Set collRegistrations = g_dbCurr.Search(strFormula, Nothing, 0)
	
	If collRegistrations.Count = 0 Then
		Call ShowErrorMsg(|Attendance update cannot be set as complete because no registration records were found marked as 'Attended'.| &amp; Chr$(13) &amp; Chr$(13) &amp; |Please mark at least one registration record as 'Attended'.|)
		Goto lblExit
	Else
		Set docDialog = g_dbCurr.CreateDocument
		docDialog.EventName = m_docCurr.eventname(0)
		
		flgOK = ws.DialogBox(|dlgMarkAttendanceComplete|, True, True, False , , , , "Mark Attendance Complete", docDialog ,True, False)
		If(Not flgOk) Then
			Call Debug(STR_MODULE_NAME &amp; |: User clicked cancel.|)
			Goto lblExit
		Else
			'Add a timestamp to indicate when the update occurred
			Set m_docCurr.ep_DateAttendanceUpdateComplete = dtNow		
			m_docCurr.ep_AttendanceUpdateCompleteBy = g_strUserName	
			m_docCurr.ep_AttendanceUpdateStatus = "Completed"
			m_docCurr.ep_AttendanceUpdateMarkedCompleteBy = g_strUserName + " on "+ Date$ + " at " + Time$
			m_docCurr.ep_IsAttendanceOverdue = ""
			m_docCurr.ep_DateAttendanceMarkedOverdue = ""		
			Call UpdateAudit(m_docCurr, g_strCommonUserName, |Attendance marked complete.|)
			Call m_docCurr.Save(True, False)
			Call m_uidocCurr.Reload
			Call m_uidocCurr.Refresh
		End If
		
	End If
	
lblExit:
	
End Sub
</lotusscript></code></actionhotspot><run><font size='9pt' name='Arial' pitch='variable'
 truetype='true' familyid='20' color='gray'/>.</run></par>
