	Resume lblExit
	
End Sub
</lotusscript></code><code event='querysave'><lotusscript>Sub Querysave(Source As Notesuidocument, Continue As Variant)
	
	Const STR_MODULE_NAME = |Event profile: Querysave|
	
	Dim strMsg As String
	Dim intCountErrors As Integer
	Dim strErrorFieldName As String
	Dim flgUseGRPHomePage As Boolean
	Dim reply As Integer
	
     'Trap errors
	On Error Goto lblError
	
	If g_dbCurr.Server="" Then
		Call ShowInfoMsg(|Local edit of this document is disabled in order to avoid replication conflicts.  Please perform your updates on the server replica.|)
		Continue = False
		Goto lblExit
	End If
	
	intCountErrors = 0
	strMsg = "The following fields are required to save this document:" &amp; Chr$(13) &amp; Chr$(13)
	
	'Validate the required fields in the order they appear on the form
	
	'If the website uses a GRP home page the validation is different from when it does not
	If (Source.FieldGetText(|ep_DefaultWebPage|) &lt;&gt; "Other page") Then
		flgUseGRPHomePage = True
	Else
		flgUseGRPHomePage = False
	End If
	
	If flgUseGRPHomePage Then
		
		If (Source.FieldGetText("offeringQuestionsName") = "") Then
			If strErrorFieldName = "" Then
				strErrorFieldName = "offeringQuestionsName"
			End If
			intCountErrors = intCountErrors + 1
			strMsg = strMsg &amp; " Event owner" &amp; Chr$(13)
		End If
		
	End If
	
	If Trim(Source.FieldGetText("brand")) = "" Then
		If strErrorFieldName = "" Then
			strErrorFieldName = "brand"
		End If
		intCountErrors = intCountErrors + 1
		strMsg = strMsg &amp; " Topic keyword(s)" &amp; Chr$(13)
	End If
	
	If (Source.FieldGetText("loc_confcity") = "") Then
		If strErrorFieldName = "" Then
			strErrorFieldName = "loc_confcity"
		End If			
		intCountErrors = intCountErrors + 1
		strMsg = strMsg &amp; " City" &amp; Chr(13)
	End If	
	
	If (Source.FieldGetText("loc_confcountry") = "") Then
		If strErrorFieldName = "" Then
			strErrorFieldName = "loc_confcountry"
		End If			
		intCountErrors = intCountErrors + 1
		strMsg = strMsg &amp; " Country" &amp; Chr(13)
	End If	
	
	If Trim(Source.FieldGetText("ep_Locales")) = "" Then
		If strErrorFieldName = "" Then
			strErrorFieldName = "ep_Locales"
		End If
		intCountErrors = intCountErrors + 1
		strMsg = strMsg &amp; " Web site languages" &amp; Chr$(13)
	End If
	
	If (m_docCurr.ep_IsMultiLingual(0) = "Yes") Then
		If Trim(Source.FieldGetText("ep_DefaultLocale")) = "" Then
			If strErrorFieldName = "" Then
				strErrorFieldName = "ep_DefaultLocale"
			End If
			intCountErrors = intCountErrors + 1
			strMsg = strMsg &amp; " Default language" &amp; Chr$(13)
		End If
	End If
	
	If flgUseGRPHomePage Then
		If (Source.FieldGetText("emailadmin") = "") Then
			If strErrorFieldName = "" Then
				strErrorFieldName = "emailadmin"
			End If			
			intCountErrors = intCountErrors + 1
			strMsg = strMsg &amp; " Contact e-mail for registration administration issues" &amp; Chr$(13)
		End If	
	End If
	'If this Canister is ON24 enabled (in the canister profile) and this Event is selected as ON24 Event (in this event profile advanced tab)
	'then require the following 5 fields to allow posting of reg data to ON24.com and receive attended status post event.
	If (m_docCurr.ep_ShowON24(0) = 1 And m_docCurr.ep_IsOn24(0)="Yes") Then
		If (m_docCurr.ep_VirtualVendor(0) = "ON24") Then
			If (m_docCurr.ep_ON24ShowCode(0) = "") Then
				If strErrorFieldName = "" Then
					strErrorFieldName = "ep_ON24ShowCode"
				End If			
				intCountErrors = intCountErrors + 1
				strMsg = strMsg &amp; " ON24 show code" &amp; Chr$(13)
			End If
			If (m_docCurr.ep_ON24ShowID(0) = "") Then
				If strErrorFieldName = "" Then
					strErrorFieldName = "ep_ON24ShowID"
				End If			
				intCountErrors = intCountErrors + 1
				strMsg = strMsg &amp; " ON24 show ID" &amp; Chr$(13)
			End If
			If (m_docCurr.ep_ON24ShowUrl(0) = "") Then
				If strErrorFieldName = "" Then
					strErrorFieldName = "ep_ON24ShowUrl"
				End If			
				intCountErrors = intCountErrors + 1
				strMsg = strMsg &amp; " ON24 show URL" &amp; Chr$(13)
			End If
			If (m_docCurr.ep_ON24ShowPassword(0) = "") Then
				If strErrorFieldName = "" Then
					strErrorFieldName = "ep_ON24ShowPassword"
				End If			
				intCountErrors = intCountErrors + 1
				strMsg = strMsg &amp; " ON24 show Password" &amp; Chr$(13)
			End If
			If (m_docCurr.ep_ON24APIKey(0) = "") Then
				If strErrorFieldName = "" Then
					strErrorFieldName = "ep_ON24APIKey"
				End If			
				intCountErrors = intCountErrors + 1
				strMsg = strMsg &amp; " ON24 show API key" &amp; Chr$(13)
			End If
		Else ' Unisfair Field validation
			If (m_docCurr.ep_UnisfairEventID(0) = "") Then
				If strErrorFieldName = "" Then
					strErrorFieldName = "ep_UnisfairEventID"
				End If
				intCountErrors = intCountErrors + 1
				strMsg = strMsg &amp; " Unisfair Event ID" &amp; Chr$(13)
			End If
			If (m_docCurr.ep_UnisfairEventSubID(0) = "") Then
				If strErrorFieldName = "" Then
					strErrorFieldName = "ep_UnisfairEventSubID"
				End If
				intCountErrors = intCountErrors + 1
				strMsg = strMsg &amp; " Unisfair Event Sub ID" &amp; Chr$(13)
			End If
		End If
		
	End If
	
	If intCountErrors &gt; 0 Then
		Call ShowErrorMsg(strMsg)
		Source.GotoField(strErrorFieldName)
		Continue = False
		Goto lblExit
	End If
	
	' 6 Dec 2011, David Holbrook - Added below
	' Give user a chance to cancel saving this event profile
	If Source.Document.STATUS(0) = "Active" Then
		reply = Messagebox ("This event is Active. Are you sure you want to save your changes?", 3 + 32, "Save?")
		If reply = 7 Or reply = 2 Then
			Continue = False
		End If
	End If
	' End of addition
	
	'Make sure that  if someone just created the event and wants to save and preview the reg page
	'that all minimum required info is there. This means create a registration profile with all
	'default values for default language (without asking to copy from another one)
	
	'So.. step 1) is it new?
'	If m_docCurr.IsNewNote Then
		'step 2)OK, it is new, but does it already have a Reg Profile for any language? 
		'(check the view by Ref ID and Locale using the default Locale for the event)
	Dim collRegProfiles As NotesDocumentCollection
	
	Set collRegProfiles = collGetRegProfilesByEventID(m_docCurr)
	
	If collRegProfiles.Count &lt; 1 Then
			'step 3)OK, it does not have any Reg Profile so create one with the default language
			' also bypass the question "do you want to copy?", because they must be in a hurry.
		Dim docRegProfile As NotesDocument
		
		Set docRegProfile = docCreateRegistrationProfile(m_docCurr,Cstr(m_docCurr.ep_DefaultLocale(0)))
		
		Call docRegProfile.Save(True,True)
		
		'Refresh the view that the document was opened from so that the status change is visible in the view
		Dim ws As New NotesUIWorkspace		
		Call ws.ViewRefresh
'		End If		
		
	End If
	
lblExit:
	
	Exit Sub
	
lblError:
	
	Call Debug(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Call LogError(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Resume lblExit
	
	
End Sub
</lotusscript></code><code event='postsave'><lotusscript>Sub Postsave(Source As Notesuidocument)
	
	Const STR_MODULE_NAME = |Event profile: Postsave|
	
	Dim flgRefreshRegistrationDocuments As Boolean
	Dim flgRefreshEventDocuments As Boolean
	Dim flgTopicsUpdated As Boolean
	
     'Trap errors
	On Error Goto lblError
	
	Call Source.RefreshHideFormulas
	
	'Check whether any registration documents need to be updated
	flgRefreshRegistrationDocuments = flgRegistrationSecurityHasChanged(m_docCurr) Or flgRegistrationDocumentsNeedUpdating(m_docCurr)
	If (flgRefreshRegistrationDocuments) Then
		'Update any existing registrations
		Call flgUpdateRegistrationDocuments(m_docCurr, True) 
	End If
	
	'Check whether any related event documents need to be updated
	flgRefreshEventDocuments = flgEventDocumentsNeedUpdating(m_docCurr)
	If (flgRefreshEventDocuments) Then
		'Update any related event documents
		Call flgUpdateEventDocuments(m_docCurr, True)
	End If	
	
	'Check whether the topics list has been updated
	flgTopicsUpdated = flgTopicListHasChanged(m_docCurr)
	
	'If any event related documents were updated or the topic list has changed, update the home database
	If (flgRefreshEventDocuments Or flgTopicsUpdated) Then
		'Update the home database (use a server based agent to do the update so that it has the correct privileges)
		Call UpdateHomeDbViaAgent(m_docCurr, INT_ADD)	
	End If
	
	'If anything has been updated, save the updated settings
	If (flgRefreshRegistrationDocuments Or flgRefreshEventDocuments Or flgTopicsUpdated) Then
		'Update the saved settings to the current settings (so we'll know if they change again)
		Call StoreInitialValues(m_docCurr)
	End If
	
	'Send to QA if an active event has been updated
	If (m_docCurr.status(0) = |Active|) Then
		'Call NotifyQATeam(m_docCurr)
	End If
	
lblExit:
	
	Exit Sub
	
lblError:
	
	Call Debug(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Call LogError(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Resume lblExit
	
End Sub
</lotusscript></code>
