	End If
	
lblExit:
	
	If (flgChanged) Then
		Call Debug(STR_MODULE_NAME &amp; |: Event documents for this event need to be updated.|)
	Else
		Call Debug(STR_MODULE_NAME &amp; |: Event documents for this event do not need to be updated.|)
	End If
	
	flgEventDocumentsNeedUpdating = flgChanged
	
	Exit Function
	
lblError:
	
	'Safest to assume event documents need updating if there's an error
	flgChanged = True
	Call Debug(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Call LogError(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Resume lblExit
	
End Function
