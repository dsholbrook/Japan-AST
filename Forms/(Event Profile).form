</lotusscript></code><code event='flgRegistrationSecurityHasChanged'><lotusscript
>Function flgRegistrationSecurityHasChanged(docEventProfile As NotesDocument) As Integer
     '------------------------------------------------------------------------------------------------------
     'Description
     '------------------
     'Return True if any of the following are true:
	'
	'	-	'Who is allowed to view registrations for this event?' has changed.
	'	-	'Users who can view registrations:' has changed.
	'
     'Parameters
     '-----------------------
     'docEventProfile				This document
     '     
     'Return Values
     '-----------------------
     'True if as above, False otherwise.
     '------------------------------------------------------------------------------------------------------   
	
	Const STR_MODULE_NAME = |Event profile: flgRegistrationSecurityHasChanged|
	
	Dim flgChanged As Integer
	Dim strCurrViewRegistrations As String
	Dim intCurrName As Integer
	
     'Trap errors
	On Error Goto lblError
	
	'Assume nothing has changed
	flgChanged = False
	
	strCurrViewRegistrations = Cstr(docEventProfile.viewRegistrations(0))
	
	'Check whether the registration security setting has changed
	flgChanged = flgFieldValueHasChanged(m_strViewRegistrations, strCurrViewRegistrations, |Who is allowed to view registrations for this event?|)
	If (flgChanged) Then
		Goto lblExit
	End If	
	
	'Check whether the list of viewers has changed
	If (Ubound(m_strarrRegistrationViewers) &lt;&gt; Ubound(docEventProfile.registrationViewers)) Then
		Call Debug(STR_MODULE_NAME &amp; |: The number of entries in 'Users who can view registrations:' has changed.|)
		flgChanged = True		
		Goto lblExit		
	End If
	
	intCurrName = 0
	Forall varName In docEventProfile.RegistrationViewers
		If (Cstr(varName) &lt;&gt; m_strarrRegistrationViewers(intCurrName)) Then
			Call Debug(STR_MODULE_NAME &amp; |: The entry at position | &amp; Cstr(intCurrName + 1)&amp; | in 'Users who can view registrations:' has changed from [| &amp; m_strarrRegistrationViewers(intCurrName)&amp; |] to [| &amp; Cstr(varName) &amp;|].|)
			flgChanged = True			
			Goto lblExit		
		End If
		intCurrName = intCurrName + 1
	End Forall
	
lblExit:
	
	If (flgChanged) Then
		Call Debug(STR_MODULE_NAME &amp; |: The registration security settings have changed.|)
	Else
		Call Debug(STR_MODULE_NAME &amp; |: The registration security settings are unchanged.|)
	End If
	
	flgRegistrationSecurityHasChanged = flgChanged
	
	Exit Function
	
lblError:
	
	'Safest to assume security has changed if there's an error
	flgChanged = True
	Call Debug(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Call LogError(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Resume lblExit
	
End Function
</lotusscript></code><code event='flgTopicListHasChanged'><lotusscript>Function flgTopicListHasChanged(docEventProfile As NotesDocument) As Boolean
