		Goto lblExit
	End If
	flgChanged = flgFieldValueHasChanged(m_strUnisfairEventID, strCurrUnisfairEventID, |Unisfair Event ID|)
	If (flgChanged) Then
		Goto lblExit
	End If
	flgChanged = flgFieldValueHasChanged(m_strVirtualVendor, strCurrVirtualVendor, |Virtual Vendor|)
	If (flgChanged) Then
		Goto lblExit
	End If
	flgChanged = flgFieldValueHasChanged(m_strUnisfairEventSubID, strCurrUnisfairEventSubID, |Unisfair Event Sub ID|)
	If (flgChanged) Then
		Goto lblExit
	End If
	
	flgChanged = flgFieldValueHasChanged(strCurrDefaultWebPage, strCurrDefaultWebPage, |Default Web Page|)
	If (flgChanged) Then
		Goto lblExit
	End If
	
	flgChanged = flgFieldValueHasChanged(m_strStatus, strCurrStatus, |Event status|)
	If (flgChanged) Then
		Goto lblExit
	End If
	
	flgChanged = flgFieldValueHasChanged(m_strEventName, strCurrEventName, |Event name|)
	If (flgChanged) Then
		Goto lblExit
	End If
	
	flgChanged = flgFieldValueHasChanged(m_strWebsiteStatus, strCurrWebsiteStatus, |Web site status|)
	If (flgChanged) Then
		Goto lblExit
	End If
	
	flgChanged = flgFieldValueHasChanged(m_strWebsiteType, strCurrWebsiteType, |Web site type|)
	If (flgChanged) Then
		Goto lblExit
	End If	
	
	flgChanged = flgFieldValueHasChanged(m_strStartDate, strCurrStartDate, |Event start date|)
	If (flgChanged) Then
