</lotusscript></code><code event='StoreInitialValues'><lotusscript>Sub StoreInitialValues(docEventProfile As NotesDocument)
     '------------------------------------------------------------------------------------------------------
     'Description 
     '----------------
     'Store the initial value of the fields that we need to know have
	'been changed during an edit session.
	'
	'NOTE:	If we need to update registrations when any additional fields
	'			change, we need to update the following:
	'
	'			1.	Change this routine to store the initial value of the additional field.
	'			2.	Change flgRegistrationDocumentsNeedUpdating() to check whether
	'				the initial value of the field has changed.
	'			3.	Change flgUpdateRegistrationDocuments() (in Common Functions) to
	'				update the registration documents with the value from the additional field.
     '
     'Parameters
     '-----------------------
     'docEventProfile				This document
     '     
     'Return Values
     '-----------------------
     'None
     '------------------------------------------------------------------------------------------------------   
	
	Const STR_MODULE_NAME = |Event profile: StoreInitialValues|
	
	Dim intCurrName As Integer
	
	'Trap errors
	On Error Goto lblError
	
	'Store the initial values of any of the fields that, if changed, mean that we need to
	'update any registration documents for this event or any associated documents such
	'as agendas, speakers, sponsors documents.
	
	m_strStatus 				= Cstr(docEventProfile.status(0))
	m_strEventName 		= Cstr(docEventProfile.eventname(0))
	m_strWebsiteStatus 	= Cstr(docEventProfile.kw_RegistrationStatus(0))
	m_strWebsiteType 		= Cstr(docEventProfile.ep_WebSiteType(0))
	m_strDefaultWebPage = Cstr(docEventProfile.ep_DefaultWebPage(0))
	m_strStartDate 			= strFormatDate(Cstr(docEventProfile.startmonth(0)), Cstr(docEventProfile.startday(0)), Cstr(docEventProfile.startyear(0)))
	m_strEndDate 			= strFormatDate(Cstr(docEventProfile.endmonth(0)), Cstr(docEventProfile.endday(0)), Cstr(docEventProfile.endyear(0)))
	m_strCity 					=Cstr(docEventProfile.loc_confcity(0))
	m_strState 				= Cstr(docEventProfile.loc_confstate(0))	
	m_strCountry 			= Cstr(docEventProfile.loc_confcountry(0))	
	m_strGeo 					= Cstr(docEventProfile.geo(0))
	m_strTactic 				= Cstr(docEventProfile.tactic(0))
	m_strBATTacticName = Cstr(docEventProfile.BATTacticname(0))
	m_strBATMPID 			= Cstr(docEventProfile.BATMPID(0))	
	m_strSurroundValues = Cstr(docEventProfile.surround_values(0))	
	'Virtual events flags
	m_strON24ShowCode		=  Cstr(docEventProfile.ep_ON24ShowCode(0))
	m_strUnisfairEventID 			=  Cstr(docEventProfile.ep_UnisfairEventID(0))
	m_strVirtualVendor				=  Cstr(docEventProfile.ep_VirtualVendor	(0))
	m_strUnisfairEventSubID	=  Cstr(docEventProfile.ep_UnisfairEventSubID(0))
	
	Redim m_strarrBrands(Ubound(docEventProfile.brand))
	
	intCurrName = 0
	
	Forall varName In docEventProfile.brand
		m_strarrBrands(intCurrName) = Cstr(varName)
		intCurrName = intCurrName + 1
	End Forall
	
	'Store the initial values of any of the security settings, that, if changed, mean that we need to
	'update any registration documents for this event.
	m_strViewRegistrations = Cstr(docEventProfile.viewRegistrations(0))
	
	Redim m_strarrRegistrationViewers(Ubound(docEventProfile.registrationViewers))
	
	intCurrName = 0
	
	Forall varName In docEventProfile.registrationViewers
		m_strarrRegistrationViewers(intCurrName) = Cstr(varName)
		intCurrName = intCurrName + 1
	End Forall
	
lblExit:
	
	Exit Sub
	
lblError:
	
	Call Debug(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Call LogError(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Resume lblExit
	
End Sub
</lotusscript></code><code event='flgFieldValueHasChanged'><lotusscript>Function flgFieldValueHasChanged(strOriginalValue As String, strCurrValue As String, strFieldTitle As String) As Boolean
	
	Const STR_MODULE_NAME = |Event profile: flgFieldHasChanged|
	
	Dim flgHasChanged As Boolean
	
	'Trap errors
	On Error Goto lblError
	
	flgHasChanged = False
	
	If (strOriginalValue &lt;&gt; strCurrValue) Then
		flgHasChanged = True
		Call Debug(STR_MODULE_NAME &amp; |: [| &amp; strFieldTitle &amp; |] has changed  from [| &amp; strOriginalValue &amp; |] to [| &amp; strCurrValue &amp; |].|)
	End If
	
lblExit:
	
	flgFieldValueHasChanged = flgHasChanged
	
	Exit Function
	
lblError:
	
	'Safest to assume field value has changed if there's an error
	flgHasChanged = True
	Call Debug(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Call LogError(STR_MODULE_NAME &amp; |: | &amp; Err() &amp; |: | &amp; Error() &amp; | in line | &amp; Erl)
	Resume lblExit
	
End Function

</lotusscript></code><code event='flgEventDocumentsNeedUpdating'><lotusscript
>Function flgEventDocumentsNeedUpdating(docEventProfile As NotesDocument) As Integer
     '------------------------------------------------------------------------------------------------------
     'Description
