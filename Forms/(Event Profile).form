		Goto lblExit
	End If
	
	flgChanged = flgFieldValueHasChanged(m_strState, strCurrState, |Event location (state)|)
	If (flgChanged) Then
		Goto lblExit
	End If
	
	flgChanged = flgFieldValueHasChanged(m_strCountry, strCurrCountry, |Event location (country)|)
	If (flgChanged) Then
		Goto lblExit
	End If
	
	flgChanged = flgFieldValueHasChanged(m_strGeo, strCurrGeo, |Event location (geo)|)
	If (flgChanged) Then
