	strCurrStartDate = strFormatDate(Cstr(docEventProfile.startmonth(0)), Cstr(docEventProfile.startday(0)), Cstr(docEventProfile.startyear(0)))
	strCurrCity =Cstr(docEventProfile.loc_confcity(0))
	strCurrState = Cstr(docEventProfile.loc_confstate(0))	
	strCurrCountry = Cstr(docEventProfile.loc_confcountry(0))	
	strCurrGeo = Cstr(docEventProfile.geo(0))
	'Virtual events flags
	strCurrON24ShowCode		=  Cstr(docEventProfile.ep_ON24ShowCode(0))
	strCurrUnisfairEventID 		=  Cstr(docEventProfile.ep_UnisfairEventID(0))
	strCurrVirtualVendor			=  Cstr(docEventProfile.ep_VirtualVendor	(0))
	strCurrUnisfairEventSubID	=  Cstr(docEventProfile.ep_UnisfairEventSubID(0))
	
	flgChanged = flgFieldValueHasChanged(m_strON24ShowCode, strCurrON24ShowCode, |ON24 Show Code|)
