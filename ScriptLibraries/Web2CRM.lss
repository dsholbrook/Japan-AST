Sub storeOptionalQsFromEIT_DB(docAREventProfile As NotesDocument, continue As Boolean)
	%REM
	Put in production on 2012.08.30
	Sub storeOptionalQsFromEIT_DB
	Called from updateAREventProfile
	Description: 'EIT Optionals, setup AR EP to handle dynamic Canister 99 QTags, Answers and Questions
	
	We know that the fields will be hard coded (see "Pattern" below) and as such we can cycle 1 thru MAX to write both
	the field name and the formula.
	
	emp_X01_QTAG will become ar_X01_QTAG with a value of whatever the QTag should be.  

Pattern:
	X01_QTAG
	X01_QUESTION
	X01_ANSWER
	%END REM
	
	
	Const STR_MODULE_NAME = |Script Library: | + STR_WEB2CRM_LIBRARY_NAME + |: storeOptionalQsFromEIT_DB|
	Call Debug(STR_MODULE_NAME & |: Started.|) 	
	On Error Goto lblError
	
	Const MAX = 30

	Dim msg As String 
	Dim i As Integer 
	Dim numchar As String
	Dim fldName As String 
	
	For i=1 To MAX Step 1
		numchar = Cstr(i)
		If i < 10 Then
			numchar = "0" + numchar
		End If
		
		fldName = "X" + numchar + "_QTAG"
		Call CopyToAREventProfile(docAREventProfile, |epm_| + fldName, fldName)	
		fldName = "X" + numchar + "_QUESTION"
		Call CopyToAREventProfile(docAREventProfile, |epm_| + fldName, fldName)	
		fldName = "X" + numchar + "_ANSWER"
		Call CopyToAREventProfile(docAREventProfile, |epm_| + fldName, fldName)	
		
		fldName = ""
	Next
	
	
lblExit:
	Call Debug(STR_MODULE_NAME & |: Finished. |) 
	Exit Sub
	
lblError:
	continue = False	
	msg = STR_MODULE_NAME & |: | & Err() & |: | & Error() & | in line | & Erl
	Print msg
	'	MsgBox  msg,, "Error"
	Call Debug(msg)
	Call LogError(msg)
	Resume lblExit	
End Sub

